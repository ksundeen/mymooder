{
    "build": {
        "preview": {
            "distribution": "internal",
            "developmentClient": true,
            "ios": {
                "enterpriseProvisioning": "adhoc"
              }        
        },
        "development": {
            "ios": {
                "simulator": true
            },
            "extends": "production",
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_API_URL": "https://localhost:8081"
            }
        },
        "test": {
            "ios": {
                "enterpriseProvisioning": "adhoc"
            },
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_API_URL": "https://mymooder.api.test.com"
                }
            },
        "production": {
            "env": {
            "EXPO_PUBLIC_API_URL": "https://mymooder.api.production.com"
            }
        }
    }
  }