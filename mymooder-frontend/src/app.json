{
  "expo": {
    "name": "My Mooder",
    "slug": "mymooder",
    "description": "My Mooder is an applicatio that aims to help transform an individualâ€™s personal view of 'mental illness' from an issue of 'mental health' to 'mood awareness' through data analytics and mood value tracking across time and space.",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/geoi.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ksundeen.mymooder"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/geoi.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ksundeen.mymooder"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo_WorldInfo_NoBackground_Facivon.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d4404ff8-8ddc-4ef1-8829-58a2ff699751"
      }
    }
  }
}
