{
  "expo": {
    "name": "My Mooder",
    "slug": "mymooder-frontend",
    "description": "My Mooder is an application that aims to help transform an individualâ€™s personal view of 'mental illness' from an issue of 'mental health' to 'mood awareness' through data analytics and mood value tracking across time and space.",
    "version": "0.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/geoi.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ksundeen.mymooderfrontend",
      "buildNumber": "15"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/geoi.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ksundeen.mymooderfrontend",
      "versionCode": 50,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/geoi_transparent.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "usesCleartextTraffic": true
          },
          "ios": {
            "deploymentTarget": "17.5"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "54b5b4f3-3e50-49f5-82bf-0e830b689170"
      }
    },
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/54b5b4f3-3e50-49f5-82bf-0e830b689170"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
